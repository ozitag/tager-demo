{
    "name": "ozitag/tager-backend",
    "type": "project",
    "description": "Tager Backend",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/passport": "^9.2",
        "sentry/sentry-laravel": "^1.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
